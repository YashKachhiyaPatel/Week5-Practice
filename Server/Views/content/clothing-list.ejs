<h1>clothing-list</h1>

<div class="row">
    <div class="col">
        <!-- table -->
        <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Brand</th>
                <th scope="col">Category</th>
                <th scope="col">Color</th>
                <th scope="col">Size</th>
                <th scope="col">Price</th>
              </tr>
            </thead>
            <tbody>
                <!-- repeating row-->
                <% for (let count = 0; count < clothing.length;  clothing++ ) { %>                    
              <tr>
                <th scope="row"><%= count + 1 %></th>
                <td><%= clothing[count].name %></td>
                <td><%= clothing[count].brand %></td>
                <td><%= clothing[count].category %></td>
                <td><%= clothing[count].color %></td>
                <td><%= clothing[count].size %></td>
                <td><%= clothing[count].price %></td>
              </tr>            
                <% } %>
             
            </tbody>
          </table>
    </div>
</div>